using namespace std;
typedef long long ll;
typedef long double ld;
typedef pair<int,int> ii;
const float eps=1e-8;
const ll lloo=1e18;
const int oo=1e9;

int main(){
    string a,b;
    cin >> a >> b;
    int lc=0;
    ford(i,a.sz-1,0) if (a[i]<'z') {
        a[i]++;
        lc=1;
        break;
    } else {
        a[i]='a';
    }
    if (!lc) {
            cout << "No such string";
            return 0;
    }
    if (a<b) cout << a;
    else cout << "No such string";
}
